$ikchain "rhand" "ValveBiped.Bip01_R_Hand" knee 0.633 0.775 0

$animation "look_down_corrective_animation" "v_desert_eagle_anims\look_down_corrective_animation.smd" {
}

$animation "look_mid_corrective_animation" "v_desert_eagle_anims\look_mid_corrective_animation.smd" {
}

$animation "look_up_corrective_animation" "v_desert_eagle_anims\look_up_corrective_animation.smd" {
}

$animation "a_idle_1" "v_desert_eagle_anims\a_idle_1.smd" {
	fps 23
	loop
	ikrule "lhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 50 50 target 0
	ikrule "rhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 50 50 target 1
}

$animation "look_mid" "v_desert_eagle_anims\look_mid.smd" {
	fps 30
	// Crowbar writes this subtract option to prevent jigglebone and poseparameter problems when delta sequences are recompiled.
	subtract "look_mid_corrective_animation" 0
}

$animation "look_down" "v_desert_eagle_anims\look_down.smd" {
	fps 30
	// Crowbar writes this subtract option to prevent jigglebone and poseparameter problems when delta sequences are recompiled.
	subtract "look_down_corrective_animation" 0
}

$animation "look_up" "v_desert_eagle_anims\look_up.smd" {
	fps 30
	// Crowbar writes this subtract option to prevent jigglebone and poseparameter problems when delta sequences are recompiled.
	subtract "look_up_corrective_animation" 0
}

$animation "a_run" "v_desert_eagle_anims\a_run.smd" {
	fps 27
	loop
	ikrule "lhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 40 40 target 0
	ikrule "rhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 40 40 target 1
}

$sequence "idle" {
	"a_run"
	"a_idle_1"
	"a_run"
	activity "ACT_VM_IDLE" 1
	blend "move_x" -1 1
	blendwidth 3
	fadein 0.2
	fadeout 0.2
	addlayer "look_poses"
}

$sequence "look_poses" {
	"look_down"
	"look_mid"
	"look_up"
	blend "ver_aims" -1 1
	blendwidth 3
	delta
	fadein 0.2
	fadeout 0.2
}

$sequence "deploy" {
	"v_desert_eagle_anims\deploy.smd"
	activity "ACT_VM_DEPLOY" 1
	fadein 0.2
	fadeout 0.2
	fps 30
	ikrule "lhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 38 38 target 0
	ikrule "rhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 38 38 target 1
}

$sequence "deploy_layer" {
	"v_desert_eagle_anims\deploy_layer.smd"
	activity "ACT_VM_DEPLOY_LAYER" 1
	{ event 5004 3 "Magnum.Deploy" }
	fadein 0.2
	fadeout 0.2
	fps 48
	ikrule "lhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 38 38 target 0
	ikrule "rhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 38 38 target 1
}

$sequence "fire" {
	"v_desert_eagle_anims\fire.smd"
	activity "ACT_VM_PRIMARYATTACK" 1
	{ event AE_MUZZLEFLASH 0 "1" }
	fadein 0.2
	fadeout 0.2
	fps 30
	ikrule "lhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 23 23 target 0
	ikrule "rhand" release contact 0 fakeorigin 0 0 0 fakerotate 0 0 0 floor 0 height 0 radius 0 range 0 0 23 23 target 1
}

$sequence "fire_layer" {
	"v_desert_eagle_anims\fire_layer.smd"
	activity "ACT_VM_PRIMARYATTACK_LAYER" 1
	{ event AE_MUZZLEFLASH 0 "1" }
	{ event AE_CL_CREATE_PARTICLE_EFFECT 1 "weapon_muzzle_flash_pistol_FP follow_attachment muzzle_flash" }
